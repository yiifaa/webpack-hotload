{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },

  "source": {
    "include": ["es6/", "index.js"],
    "includePattern": ".+\\.js(doc)|es6?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },

  "plugins": [],

  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },

  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "docs/api",
    "recurse": true,
    "tutorials": "docs/tutorials"
  }
}